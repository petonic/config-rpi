#! /bin/bash
#
#

ttydev=/dev/tty1

sudo chmod 666 $ttydev
echo -ne "\033[9;0]" > $ttydev

cat > $ttydev <<-EOF
__    _
( /   /     /       _/_ /
/ / /__,  /<  _    /  /_  _
(_/_/(_/(_/ |_(/_  (__/ /_(/_


________   , ,___ __  ,
(  /  ( /  / /   /( /,/               ///
-/--  /  / /      /<      , ,  ,_   ///
_/    (_,/_(___/  /  \_   (_/__/|_)_'''
                              /|  ooo
                             (/

EOF
