# Don't print warnings out.
LogLevel ERROR

#
# From https://github.com/ginatrapani/todo.txt-cli/wiki/Tips-and-Tricks
#   This causes ssh to use the existing connection rather than opening new
#   ones. The speed up for additional connections is amazing.
Host *
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
    IdentityFile ~/.ssh/id_rsa



Host rp rp.local
    Hostname rp.local
     User pi

Host rp3 rp3.local
    Hostname rp3.local
    User pi

Host pz1 pz1.local
    Hostname pz1.local
    User pi

Host simtel simtel20 simtel20.local simtel.local
    Hostname simtel20.local
    User root

Host pithy pithy.local
    Hostname pithy.local
    RemoteForward   52698 127.0.0.1:52698
    User pi

Host cellarmon cellarmon.local
    Hostname cellarmon.local
    User pi

Host pihole.local pihole ph
    Hostname pihole.local
    User pi

Host pivm.local pivm pv
    Hostname pivm.local
    User pi


Host kaypro kaypro.local
    Hostname kaypro.local
    User petonic


Host karenspi karenspi.local kpi kpi.local
    Hostname karenspi.local
    User pi

Host sfodemo sfodemo.local sfo sfo.local
    Hostname sfodemo.local
    User pi
#
Host serf1 serf1.local
    Hostname serf1.local
    User pi


